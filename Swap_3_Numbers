#include <iostream>
using namespace std;

int main()
{
    int a = 2, b = 6, c = 5, temp;

    cout << "Before swapping." << endl;
    cout << "a = " << a << ", b = " << b << ", c = " << c << endl;

    temp = a;
    a = b;
    b = c;
    c = temp;

    cout << "\nAfter swapping." << endl;
    cout << "a = " << a << ", b = " << b << ", c = " << c << endl;

    return 0;
}